#!/bin/bash

packages=(
  "appcleaner"
  "atom"
  "authy"
  "dashlane"
  "docker"
  "dropbox"
  "firefox"
  "firefox-developer-edition"
  "github-desktop"
  "google-chrome"
  "google-chrome-canary"
  "iina"
  "kaleidoscope"
  "kitematic"
  "rdm"
  "robo-3t"
  "spotify"
  "sublime-text"
  "vlc"
  "virtualbox"
  "torbrowser"
  "visual-studio-code"
)

for package in "${packages[@]}"
do
  printf "\e[1;32m\nInstalling \e[1;34m$package\e[0m\n"
  echo ""
  brew cask install $package
done

exit 0
